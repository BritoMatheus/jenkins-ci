pipeline {
    agent any

    stages {
        
        stage ('Build') {
            steps {
                dir('src\\Jenkins.CICD\\Jenkins.CICD') {
                    bat 'dotnet build '
                }
            }
        }

        stage ('Test') {
            steps {
                dir('src\\Jenkins.CICD\\Jenkins.CICD.Test') {
                    bat 'dotnet test '
                }
            }
        }

        stage ('Meio') {
            steps {
                bat 'echo meio'
                sleep(5)
            }
        }
        
        stage ('Fim') {
            steps {
                bat 'echo fim'
            }
        }
        
    }
}